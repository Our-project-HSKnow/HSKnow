<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bookmark_history_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hsknows.accountFragment.Account_history_bookmarkFragment">

    <com.getbase.floatingactionbutton.FloatingActionsMenu
        android:id="@+id/bookmark_history_floatingbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="9dp"
        app:fab_addButtonColorNormal="@color/background"
        app:fab_icon="@drawable/ic_floatingmenu_add"
        app:fab_labelStyle="@style/menu_labels_style"
        app:fab_labelsPosition="left"
        app:fab_size="mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/history_floatingbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:fab_colorNormal="@color/background"
            app:fab_icon="@drawable/ic_account_history"
            app:fab_size="mini"
            app:fab_title="@string/history" />

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/bookmark_floatingbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:fab_colorNormal="@color/background"
            app:fab_icon="@drawable/ic_account_bookmark"
            app:fab_size="mini"
            app:fab_title="@string/bookmark" />
    </com.getbase.floatingactionbutton.FloatingActionsMenu>
</androidx.constraintlayout.widget.ConstraintLayout>